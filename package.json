{
    "name": "arbitrage-bot-final",
    "version": "1.0.0",
    "description": "High-performance cryptocurrency arbitrage bot with real-time monitoring",
    "main": "index.js",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "node index.js",
        "dev": "node --watch index.js",
        "clean": "node -e \"import('fs').then(fs=>fs.rmSync('dist',{recursive:true,force:true}))\"",
        "build": "npm run clean && node scripts/build.js",
        "start:dist": "node dist/index.js",
        "build:protect": "npm run build && node scripts/obfuscate.js",
        "start:protected": "node dist_protected/index.js",
        "test": "echo \"No tests specified\" && exit 0",
        "lint": "echo \"No linter configured\" && exit 0"
    },
    "keywords": [
        "arbitrage",
        "cryptocurrency",
        "trading",
        "bot",
        "mexc",
        "lbank",
        "kcex",
        "ourbit",
        "xt",
        "dexscreener"
    ],
    "author": "Arbitrage Bot Team",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.11.0",
        "bottleneck": "^2.19.5",
        "ccxt": "^4.4.99",
        "chalk": "^5.5.0",
        "decimal.js": "^10.6.0",
        "dotenv": "^17.2.1",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "jalaali-js": "^1.2.8",
        "log-update": "^6.1.0",
        "node-cron": "^4.2.1",
        "p-limit": "^7.1.0",
        "p-queue": "^8.1.0",
        "puppeteer": "^24.16.0",
        "puppeteer-core": "^24.16.0",
        "socket.io": "^4.7.4",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "javascript-obfuscator": "^4.1.0"
    },
    "engines": {
        "node": ">=18",
        "npm": ">=8"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/arbitrage-bot-final.git"
    },
    "bugs": {
        "url": "https://github.com/your-username/arbitrage-bot-final/issues"
    },
    "homepage": "https://github.com/your-username/arbitrage-bot-final#readme"
}